model TreatmentHistory{
    id                Int        @id @default(autoincrement())
    animalId          Int        @map("animal_id")
    animal            Animal     @relation(fields: [animalId], references: [id])
    treatment         Treatment?
    confirmationDate  DateTime?
    confirmed         Boolean
    visitDate         DateTime?
    nextVisitDate     DateTime?

    @@map("treatment_history")
}