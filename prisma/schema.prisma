generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
  previewFeatures = ["relationJoins"]
  binaryTargers = ["native", "debian-openssl-1.1.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
