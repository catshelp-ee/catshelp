{
  "name": "ch-veebileht",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:api dev:vite",
    "dev:api": "nodemon --watch server --ext ts,tsx --exec tsx server/main.ts",
    "dev:vite": "vite",
    "copy:pm2": "cp server/pm2.config.js dist/",
    "copy:prisma": "npx prisma generate && cp -r prisma dist/prisma && cp -r generated dist/generated && cp -r node_modules/.prisma/client dist/generated/prisma",
    "build": "vite build && npm run bundle --prefix ./server",
    "lint": "eslint server --ext .ts",
    "start": "node dist/bundle.cjs"
  },
  "prisma": {
    "schema": "./prisma"
  },
