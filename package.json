{
  "name": "catshelp",
  "packageManager": "npm@10.8.2",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build:server": "turbo run build --filter=@catshelp/server",
    "build": "turbo run build",
    "start:prod": "node build/server/main.js",
    "client": "npm run dev:vite -w @catshelp/client",
    "server": "cross-env DEV_SERVER=true npm run start:dev -w @catshelp/server",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migration:run": "cross-env node ./node_modules/typeorm/cli.js migration:run -d build/server/data-source.js",
    "migration:create": "cross-var npm run typeorm -- migration:create ./migrations/$npm_config_name"
  },
  "dependencies": {
    "@nestjs/schedule": "^6.0.1",
    "cross-env": "^10.1.0",
    "esbuild": "^0.25.11"
  },
  "devDependencies": {
    "cross-var": "^1.1.0",
    "turbo": "^2.5.8"
  }
}
