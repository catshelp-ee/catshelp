{
  "imports": {
    "@oak/oak": "jsr:@oak/oak@^17.1.2",
    "@std/path": "jsr:@std/path@^1.0.8",
    "@tajpouria/cors": "jsr:@tajpouria/cors@^1.2.1",
    "@/": "./src/",
    "@analytics/": "./src/analytics/",
    "@assets/": "./src/assets/",
    "@context/": "./src/context/",
    "@pages/": "./src/pages/",
    "@style/": "./src/style/",
    "@models/": "./src/types/",
    "@utils/": "./src/utils/",
    "@hooks/": "./src/hooks/"
  },
  "tasks": {
    "dev": "deno task dev:api & deno task dev:vite",
    "dev:api": "deno run --allow-env --allow-net --allow-read --allow-write --allow-sys --import-map=import_map.json server/main.ts",
    "dev:vite": "deno run -A npm:vite",
    "build": "deno run --allow-env --allow-read --allow-write --allow-ffi --allow-sys --allow-run npm:vite build",
    "lint": "npm run lint",
    "lint:check": "npm run lint",
    "lint:fix": "npm run lint",
    "preview": "deno run -A npm:vite preview",
    "serve": "deno task build && deno task dev:api"
  }
}
