{
  "name": "ch-veebileht",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "deno task dev:api & deno task dev:vite",
    "dev:api": "deno run --allow-env --allow-net --allow-read --allow-write --allow-sys --import-map=import_map.json server/main.ts",
    "dev:vite": "deno run -A npm:vite",
    "build": "deno run --allow-env --allow-read --allow-write --allow-ffi --allow-sys --allow-run npm:vite build ",
    "lint": "tsc-b && eslint .",
    "preview": "vite preview",
    "serve": "deno task build && deno task dev:api",
    "deploy": "deno run -A npm:vite build && deno compile --allow-env --allow-net --allow-read --allow-write --allow-sys --output dist/server server/main.ts",
    "start": "cd dist && ./server"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.6",
    "@mui/x-date-pickers": "^7.22.1",
    "@react-oauth/google": "^0.12.1",
    "@types/node": "^22.8.7",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.5.1",
    "google-auth-library": "^9.15.1",
    "googleapis": "^144.0.0",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "mariadb": "^3.4.0",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.16",
    "postcss": "^8.4.47",
    "posthog-js": "^1.236.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-jwt": "^1.2.2",
    "react-oauth": "^0.0.1",
    "react-router-dom": "^6.27.0",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2",
    "tailwindcss": "^3.4.14",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "esbuild": "^0.25.5",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10"
  }
}
