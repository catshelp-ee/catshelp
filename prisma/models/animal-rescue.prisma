model AnimalRescue {
  id                 Int                       @id @default(autoincrement())
  rescueDate         DateTime?                 @map("rescue_date")
  state              String?
  address            String?
  locationNotes      String?                   @map("location_notes")
  rankNr             String?                   @map("rank_nr") @unique
  animalsToRescue    AnimalToAnimalRescue[]
  
  @@map("animal_rescues")
}